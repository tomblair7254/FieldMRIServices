@page "/"
@inject Microsoft.AspNetCore.Antiforgery.IAntiforgery Xsrf
@namespace FieldMRIServicesServices.Pages
@using FieldMRIServices
@using FieldMRIServicesServices.Data
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "_Layout";

    var tokens = new TokenProvider
    {
        XsrfToken = Xsrf.GetAndStoreTokens(HttpContext).RequestToken
    };

}
<script src="~/js/sessionTimeout.js"></script>
<script src="~/js/index.js"></script>
<script src="~/js/customStyles.js"></script>
<script src="~/js/scroll.js"></script>
<script>
    function setPageTitle(title) {
        document.title = title;
    }
</script>
<link
    href="https://api.fontshare.com/v2/css?f[]=excon@100,300,400,500,700,900&f[]=alpino@100,300,400,500,700,900&display=swap"
    rel="stylesheet">
<link href="https://cdn.syncfusion.com/blazor/26.2.4/styles/tailwind.css" rel="stylesheet" />
<component type="typeof(App)" param-InitialState="tokens" render-mode="ServerPrerendered" />